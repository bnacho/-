   "source": [
    "#  입력함수(input())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "44b2881d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "이름 입력 : 홍길동\n",
      "홍길동\n",
      "나이 입력 : 123\n",
      "123\n",
      "123 <class 'str'>\n",
      "143\n"
     ]
    }
   ],
   "source": [
    "# 사용자로부터 입력을 받아야 할 때 입력함수를 사용한다.\n",
    "# 형식 : input('입력받아야 하는 값의 정보')\n",
    "\n",
    "# 입력함수는 입력받은 값을 반환하는 함수로, print()나 변수를 이용해줘야한다.\n",
    "print(input('이름 입력 : '))\n",
    "age = input('나이 입력 : ')\n",
    "print(age)\n",
    "\n",
    "# 입력받은 값은 문자열로 반환되므로 연산이 필요할 때는 형변환을 해줘야한다.\n",
    "print(age, type(age))\n",
    "print(int(age) + 20)"
   ]
  },
